{
  "do_nothing": {
    "uses": "flow",
    "spec": "do_nothing {why%reason?text}",
    "slots": "execute",
    "desc": "Statement which does nothing."
  },
  "log": {
    "uses": "flow",
    "spec": "log {_%value:assignment} {as%level?logging_level}",
    "slots": "execute",
    "desc": "Debug log"
  },
  "logging_level": {
    "uses": "str",
    "spec": "{note} {to_do} {fix} {info} {warning} {error}"
  }
}

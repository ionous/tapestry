                  },
                  {
                    "Term:type:optional:": [
                      "class",
                      "text",
                      true
                    ]
                  }
                ]
              ]
            }
          ]
        },
        {
          "--": "Text List: Specifies a set of text values.",
          "Spec:slots:with flow:": [
            "text_values",
            [
              "text_list_eval",
              "literal_value"
            ],
            {
              "Flow:uses:": [
                "txts",
                [
                  {
                    "Term:name:type:repeats:": [
                      "_",
                      "values",
                      "text",
                      true
                    ]
                  },
                  {
                    "Term:type:optional:": [
                      "class",
                      "text",
                      true
                    ]
                  }
                ]
              ]
            }
          ]
        }
      ]
    }
  ]
}

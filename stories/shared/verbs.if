      "--": [
        "adapts a verb from its specified (usually present) tense, to the story's tense.",
        "fix: this is just a placeholder for future work."
      ],
      "Define pattern:requires:provides:do:": [
        "verbing",
        [
          {
            "Text:": "verb"
          }
        ],
        {
          "Text:": "result"
        },
        {
          "Set:value:": [
            "@result",
            {
              "FromText:": "@verb"
            }
          ]
        }
      ]
    },

{
  "Tapestry:": [
    {
      "Pattern:requires:returns:": [
        "verbing",
        [
          {
            "Text:": "verb"
          }
        ],
        {
          "PatternResult:": {
            "Text:": "result"
          }
        }
      ]
    },
    {
      "Comment:": "verb adapts a verb from its specified (usually present) tense, to the story's tense. [ this is just a placeholder for future work. ] "
    },
    {
      "Pattern:rules:": [
        "verbing",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "Set:from:": [
                    "@result",
                    {
                      "FromText:": "@verb"
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:requires:": [
        "contain",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "contain",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "If:does:else:": [
                    {
                      "KindOf:is:": [
                        "@noun",
                        "containers"
                      ]
                    },
                    [
                      {
                        "Print:": "contain"
                      }
                    ],
                    {
                      "ElseDo does:": [
                        {
                          "Print:": "support"
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:requires:": [
        "On",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "On",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "If:does:else:": [
                    {
                      "KindOf:is:": [
                        "@noun",
                        "containers"
                      ]
                    },
                    [
                      {
                        "Print:": "in"
                      }
                    ],
                    {
                      "ElseDo does:": [
                        {
                          "Print:": "on"
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:requires:": [
        "Off",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "Off",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "If:does:else:": [
                    {
                      "KindOf:is:": [
                        "@noun",
                        "containers"
                      ]
                    },
                    [
                      {
                        "Print:": "out"
                      }
                    ],
                    {
                      "ElseDo does:": [
                        {
                          "Print:": "off"
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:requires:": [
        "Are",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          },
          {
            "TextList:": "names"
          }
        ]
      ]
    },
    {
      "Comment:": "print \"is\" or \"are\" depending on the plurality of the noun, and (optionally) the length of the passed list of names. "
    },
    {
      "Pattern:rules:": [
        "Are",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "If:does:else:": [
                    {
                      "AnyTrue:": [
                        {
                          "Cmp:is:num:": [
                            {
                              "Len:": {
                                "FromTextList:": "@names"
                              }
                            },
                            "greater_than",
                            1
                          ]
                        },
                        {
                          "Object:field:": [
                            "@noun",
                            "plural named"
                          ]
                        }
                      ]
                    },
                    [
                      {
                        "Print:": "are"
                      }
                    ],
                    {
                      "ElseDo does:": [
                        {
                          "Print:": "is"
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": "@noun"
                }
              },
              [
                {
                  "Print:": "are"
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Comment:": "say the phrase of are not or is not ( depending on the plural of the noun )"
    },
    {
      "Pattern:requires:": [
        "Are not",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "Are not",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "Print:": "is not"
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "Object:field:": [
                  "@noun",
                  "plural named"
                ]
              },
              [
                {
                  "Print:": "are not"
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": "@noun"
                }
              },
              [
                {
                  "Print:": "are not"
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "--": "Say the contraction aren't or isn't ( depending on the plural of the noun ).",
      "Pattern:requires:": [
        "Arent",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "Arent",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "Print:": "isn't"
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "Object:field:": [
                  "@noun",
                  "plural named"
                ]
              },
              [
                {
                  "Print:": "aren't"
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": "@noun"
                }
              },
              [
                {
                  "Print:": "aren't"
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": "Say the contraction don't or doesn't ( depending on the plural of the noun ).",
      "Pattern:requires:": [
        "Dont",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "Dont",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "Print:": "doesn't"
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "Object:field:": [
                  "@noun",
                  "plural named"
                ]
              },
              [
                {
                  "Print:": "don't"
                }
              ]
            ]
          }
        ]
      ]
    }
  ]
}

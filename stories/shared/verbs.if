      "--": ""
    },
    {
      "--": "print contain or support depending on the kind of container",
      "Define pattern:requires:provides:do:": [
        "Contain",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          }
        ],
        {
          "Nothing": true
        },
        {
          "If:do:else:": [
            {
              "KindOf:is:": [
                "@noun",
                "containers"
              ]
            },
            [
              {
                "Print:": "contain"
              }
            ],
            {
              "Finally do:": [
                {
                  "Print:": "support"
                }
              ]
            }
          ]
        }
      ]
    },

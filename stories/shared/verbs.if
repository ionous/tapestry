    {
      "--": "print \"is\" or \"are\" depending on the plurality of the noun, and (optionally) the length of the passed list of names.",
      "Define pattern:requires:provides:do:": [
        "Are",
        [
          {
            "Text:kind:": [
              "noun",
              "object"
            ]
          },
          {
            "TextList:": "names"
          }
        ],
        {
          "Nothing": true
        },
        {
          "If:do:else:": [
            {
              "IsPlayer obj:": {
                "FromText:": "@noun"
              }
            },
            {
              "Print:": "are"
            },
            {
              "If:do:else:": [
                {
                  "AnyTrue:": [
                    {
                      "Is:matching:num:": [
                        {
                          "Len:": {
                            "FromTextList:": "@names"
                          }
                        },
                        "greater_than",
                        1
                      ]
                    },
                    {
                      "Object:field:": [
                        "@noun",
                        "plural named"
                      ]
                    }
                  ]
                },
                {
                  "Print:": "are"
                },
                {
                  "Finally do:": [
                    {
                      "Print:": "is"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "--": ""
    },

{
  "Tapestry:": [
    {
      "GrammarDecl:": {
        "Directive:scans:": [
          [
            "jump",
            "skip",
            "hop"
          ],
          [
            {
              "As:": "jumping"
            }
          ]
        ]
      }
    },
    {
      "--": ""
    },
    {
      "Event:action:args none:": [
        "jump",
        "jumping",
        "nothing"
      ]
    },
    {
      "Extend pattern:provides:withRules:": [
        "jumping",
        [
          {
            "Text:kind:initially:": [
              "actor",
              "object",
              {
                "Object:field:": [
                  "@agent",
                  "pawn"
                ]
              }
            ]
          }
        ],
        [
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": "@actor"
                }
              },
              [
                {
                  "ReportJumping actor:": {
                    "FromText:": "@actor"
                  }
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "report jumping",
        [
          {
            "Text:kind:": [
              "actor",
              "actor"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Extend pattern:withRules:": [
        "report jumping",
        [
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": "@actor"
                }
              },
              [
                {
                  "Print response:with:": [
                    "player jumping",
                    {
                      "Say:": "{we:|capitalize!} jump on the spot."
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "Print response:with:": [
                    "actor jumping",
                    {
                      "Say:": "{the:.actor|capitalize!} jumps on the spot."
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    }
  ]
}

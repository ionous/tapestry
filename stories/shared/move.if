{
  "Tapestry:": [
    {
      "Define pattern:requires:result:withRules:": [
        "Move Object",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          },
          {
            "Text:kind:": [
              "dest",
              "object"
            ]
          }
        ],
        {
          "Text:": "response"
        },
        []
      ]
    },
    {
      "Extend pattern:withRules:": [
        "Move Object",
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "If:do:else:": [
                    {
                      "Is empty:": "@obj"
                    },
                    [
                      {
                        "Set:from:": [
                          "@response",
                          {
                            "FromText:": {
                              "Print response:with:": [
                                "You can't move nothing.",
                                {
                                  "Say:": "You can't move nothing."
                                }
                              ]
                            }
                          }
                        ]
                      }
                    ],
                    {
                      "Else if:do:else:": [
                        {
                          "Not:": {
                            "KindOf:is:": [
                              "@obj",
                              "things"
                            ]
                          }
                        },
                        [
                          {
                            "Set:from:": [
                              "@response",
                              {
                                "FromText:": {
                                  "Print response:with:": [
                                    "You can only move things.",
                                    {
                                      "Say:": "You can only move things."
                                    }
                                  ]
                                }
                              }
                            ]
                          }
                        ],
                        {
                          "Else do:": [
                            {
                              "If:do:": [
                                {
                                  "Object:field:": [
                                    "@obj",
                                    "worn"
                                  ]
                                },
                                [
                                  {
                                    "Set:from:": [
                                      {
                                        "Object:field:": [
                                          "@obj",
                                          "not worn"
                                        ]
                                      },
                                      {
                                        "FromBool:": true
                                      }
                                    ]
                                  }
                                ]
                              ]
                            },
                            {
                              "If:do:": [
                                {
                                  "Cmp:is:txt:": [
                                    "@dest",
                                    "other_than",
                                    {
                                      "ParentOf obj:": {
                                        "FromText:": "@obj"
                                      }
                                    }
                                  ]
                                },
                                [
                                  {
                                    "Relate:to:via:": [
                                      "@dest",
                                      "@obj",
                                      "whereabouts"
                                    ]
                                  }
                                ]
                              ]
                            },
                            {
                              "Set:from:": [
                                "@response",
                                {
                                  "FromText:": {
                                    "Print response:with:": [
                                      "okay",
                                      ""
                                    ]
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "Remove from play",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Extend pattern:withRules:": [
        "Remove from play",
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "If:do:": [
                    {
                      "Is empty:": "@from"
                    },
                    [
                      {
                        "Log:value:": [
                          "debug",
                          {
                            "FromText:": "You can't remove nothing from play."
                          }
                        ]
                      }
                    ]
                  ]
                },
                {
                  "Set:from:": [
                    {
                      "Object:field:": [
                        "@obj",
                        "not worn"
                      ]
                    },
                    {
                      "FromBool:": true
                    }
                  ]
                },
                {
                  "Relate:to:via:": [
                    "",
                    "@from",
                    "whereabouts"
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "Wear object",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          },
          {
            "Text:kind:": [
              "actor",
              "actor"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Comment:": "This is pretty low level. For instance, it doesnt check wearable."
    },
    {
      "Extend pattern:withRules:": [
        "Wear object",
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "If:do:": [
                    {
                      "Not:": {
                        "Cmp:is:txt:": [
                          {
                            "HolderOf obj:": {
                              "FromText:": "@obj"
                            }
                          },
                          "equal_to",
                          "@actor"
                        ]
                      }
                    },
                    [
                      {
                        "MoveObject obj:dest:": [
                          {
                            "FromText:": "@obj"
                          },
                          {
                            "FromText:": "@actor"
                          }
                        ]
                      }
                    ]
                  ]
                },
                {
                  "If:do:": [
                    {
                      "Cmp:is:txt:": [
                        {
                          "HolderOf obj:": {
                            "FromText:": "@obj"
                          }
                        },
                        "equal_to",
                        "@actor"
                      ]
                    },
                    [
                      {
                        "Set:from:": [
                          {
                            "Object:field:": [
                              "@obj",
                              "not worn"
                            ]
                          },
                          {
                            "FromBool:": true
                          }
                        ]
                      }
                    ]
                  ]
                }
              ]
            ]
          }
        ]
      ]
    }
  ]
}

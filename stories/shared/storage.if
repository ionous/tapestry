      },
      {
        "Pattern:rules:": [
          "allows storage",
          {
            "PatternRules:": [
              {
                "PatternRule:hook activity:": [
                  {
                    "ProvidesStorage obj:": "@obj"
                  },
                  {
                    "Act:": [
                      {
                        "Let:be:": [
                          "success",

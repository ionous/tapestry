{
  "Tapestry:": {
    "Story:": [
      {
        "Pattern:requires:returns:": [
          "provides storage",
          {
            "PatternParams:": [
              {
                "Text named:of:": [
                  "obj",
                  "object"
                ]
              }
            ]
          },
          {
            "PatternResult:": {
              "Bool named:": "success"
            }
          }
        ]
      },
      {
        "Comment:": "Does the object allow items to be stored inside or on top of it? "
      },
      {
        "Pattern:rules:": [
          "provides storage",
          {
            "PatternRules:": [
              {
                "PatternRule:hook activity:": [
                  {
                    "KindOf:is:": [
                      "@obj",
                      "containers"
                    ]
                  },
                  {
                    "Act:": [
                      {
                        "Let:be:": [
                          "success",
                          {
                            "FromBool:": true
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "PatternRule:hook activity:": [
                  {
                    "KindOf:is:": [
                      "@obj",
                      "supporters"
                    ]
                  },
                  {
                    "Act:": [
                      {
                        "Let:be:": [
                          "success",
                          {
                            "FromBool:": true
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "--": ""
      },
      {
        "Pattern:requires:returns:": [
          "shows storage",
          {
            "PatternParams:": [
              {
                "Text named:of:": [
                  "obj",
                  "object"
                ]
              }
            ]
          },
          {
            "PatternResult:": {
              "Bool named:": "success"
            }
          }
        ]
      },
      {
        "Comment:": "Does the object provide storage, and can that storage be seen from outside? "
      },
      {
        "Pattern:rules:": [
          "shows storage",
          {
            "PatternRules:": [
              {
                "PatternRule:hook activity:": [
                  {
                    "ProvidesStorage obj:": "@obj"
                  },
                  {
                    "Act:": [
                      {
                        "Let:be:": [
                          "success",
                          {
                            "FromBool:": {
                              "Not:": {
                                "FullyOpaque obj:": "@obj"
                              }
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "--": ""
      },
      {
        "Pattern:requires:returns:": [
          "visible storage",
          {
            "PatternParams:": [
              {
                "Text named:of:": [
                  "obj",
                  "thing"
                ]
              }
            ]
          },
          {
            "PatternResult:": {
              "TextList named:": "out"
            }
          }
        ]
      },
      {
        "Comment:": "Return a list containing any visible contents."
      },
      {
        "Pattern:rules:": [
          "visible storage",
          {
            "PatternRules:": [
              {
                "PatternRule:hook activity:": [
                  {
                    "ShowsStorage obj:": "@obj"
                  },
                  {
                    "Act:": [
                      {
                        "Repeating across:as:do:": [
                          {
                            "FromTxts:": {
                              "ChildrenOf obj:": "@obj"
                            }
                          },
                          {
                            "AsTxt:": "it"
                          },
                          {
                            "Act:": [
                              {
                                "If:do:": [
                                  {
                                    "Not:": {
                                      "AnyTrue:": [
                                        {
                                          "Cmp:is:txt:": [
                                            "@it",
                                            {
                                              "Equals": true
                                            },
                                            {
                                              "Get:from:": [
                                                "pawn",
                                                {
                                                  "ObjFields:": "player"
                                                }
                                              ]
                                            }
                                          ]
                                        },
                                        {
                                          "Get obj:trait:": [
                                            "@it",
                                            "scenery"
                                          ]
                                        }
                                      ]
                                    }
                                  },
                                  {
                                    "Act:": [
                                      {
                                        "Put:into:": [
                                          "@it",
                                          {
                                            "IntoTxts:": "out"
                                          }
                                        ]
                                      }
                                    ]
                                  }
                                ]
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {

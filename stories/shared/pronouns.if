{
  "Tapestry:": [
    {
      "Pattern:requires:": [
        "We",
        [
          {
            "TextList:": "names"
          }
        ]
      ]
    },
    {
      "Comment:": "We refers to the player. As with all the customizable grammar, we use the plural for the name of pattern."
    },
    {
      "Pattern:rules:": [
        "We",
        [
          {
            "PatternRule:does:": [
              {
                "Always": true
              },

{
    "Tapestry:":
    [
        {
            "Pattern:requires:returns:":
            [
                "starts with vowel",
                {
                    "PatternParams:":
                    [
                        {
                            "Text named:": "text"
                        }
                    ]
                },
                {
                    "PatternResult:":
                    {
                        "Bool named:": "matching"
                    }
                }
            ]
        },
        {
            "Comment:": "Determine if text starts with a vowel or vowel sound. This method is from mudconnect.com"
        },
        {
            "--": ""
        },

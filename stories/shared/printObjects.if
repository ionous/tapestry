{
  "Tapestry:": [
    {
      "Define pattern:requires:result:withRules:": [
        "print inline objects",
        [
          {
            "TextList:": "objs"
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Extend pattern:provides:withRules:": [
        "print inline objects",
        [
          {
            "RecordList:kind:initially:": [
              "groups",
              "object group",
              {
                "MakeGroups objs:": {
                  "FromTextList:": "@objs"
                }
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "Print:": {
                    "Commas do:": [
                      {
                        "Repeating across:as:do:": [
                          {
                            "FromRecordList:": "@groups"
                          },
                          "group",
                          [
                            {
                              "Print:": {
                                "Spaces do:": [
                                  {
                                    "PrintGroup group:": {
                                      "FromRecord:": "@group"
                                    }
                                  }
                                ]
                              }
                            }
                          ]
                        ]
                      }
                    ]
                  }
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print block list",
        [
          {
            "TextList:": "objs"
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Extend pattern:provides:withRules:": [
        "print block list",
        [
          {
            "RecordList:kind:initially:": [
              "groups",
              "object group",
              {
                "MakeGroups objs:": {
                  "FromTextList:": "@objs"
                }
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "Print:": {
                    "Rows do:": [
                      {
                        "Repeating across:as:do:": [
                          {
                            "FromRecordList:": "@groups"
                          },
                          "group",
                          [
                            {
                              "Print:": {
                                "Row do:": [
                                  {
                                    "PrintGroup group:": {
                                      "FromRecord:": "@group"
                                    }
                                  }
                                ]
                              }
                            }
                          ]
                        ]
                      }
                    ]
                  }
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print object",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Comment:": "Say the object's name ( and some object details. ) "
    },
    {
      "Extend pattern:provides:withRules:": [
        "print object",
        [
          {
            "Number:initially:": [
              "inline depth",
              {
                "During:": "print inline objects"
              }
            ]
          },
          {
            "Number:initially:": [
              "block depth",
              {
                "During:": "print block list"
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:num:": [
                      "@block_depth",
                      "greater_than",
                      0
                    ]
                  },
                  {
                    "Cmp:is:num:": [
                      "@block_depth",
                      "greater_than",
                      "@inline_depth"
                    ]
                  }
                ]
              },
              [
                {
                  "PrintOneBlock obj:": {
                    "FromText:": "@obj"
                  }
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:num:": [
                      "@inline_depth",
                      "greater_than",
                      0
                    ]
                  },
                  {
                    "Cmp:is:num:": [
                      "@inline_depth",
                      "greater_than",
                      "@block_depth"
                    ]
                  }
                ]
              },
              [
                {
                  "Print:": {
                    "Spaces do:": [
                      {
                        "A noun:": {
                          "FromText:": "@obj"
                        }
                      },
                      {
                        "Print:": {
                          "Brackets do:": [
                            {
                              "PrintDetails obj:": {
                                "FromText:": "@obj"
                              }
                            }
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "Say:": "{.obj}"
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print one block",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Extend pattern:provides:withRules:": [
        "print one block",
        [
          {
            "TextList:initially:": [
              "kids",
              {
                "VisibleStorage obj:": {
                  "FromText:": "@obj"
                }
              }
            ]
          },
          {
            "Bool:initially:": [
              "has kids",
              {
                "Cmp:is:num:": [
                  {
                    "Len:": {
                      "FromTextList:": "@kids"
                    }
                  },
                  "greater_than",
                  0
                ]
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "Print:": {
                    "Spaces do:": [
                      {
                        "A noun:": {
                          "FromText:": "@obj"
                        }
                      },
                      {
                        "PrintStatus obj:isEmpty:": [
                          {
                            "FromText:": "@obj"
                          },
                          {
                            "FromBool:": {
                              "Not:": "@has_kids"
                            }
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "If:do:": [
                    "@has_kids",
                    [
                      {
                        "PrintBlockList objs:": {
                          "FromTextList:": "@kids"
                        }
                      }
                    ]
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print inline storage",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          }
        ],
        {
          "Bool:": "success"
        },
        []
      ]
    },
    {
      "Extend pattern:provides:withRules:": [
        "print inline storage",
        [
          {
            "TextList:initially:": [
              "kids",
              {
                "VisibleStorage obj:": {
                  "FromText:": "@obj"
                }
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              {
                "Cmp:is:num:": [
                  {
                    "Len:": {
                      "FromTextList:": "@kids"
                    }
                  },
                  "greater_than",
                  0
                ]
              },
              [
                {
                  "Print response:with:": [
                    "inline storage",
                    {
                      "Say:": "{On: .obj|capitalize!} {the: .obj} {are: .obj .kids} {print_inline_objects: .kids}."
                    }
                  ]
                },
                {
                  "Set:from:": [
                    "@success",
                    {
                      "FromBool:": true
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print details",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          }
        ],
        {
          "Nothing": true
        },
        []
      ]
    },
    {
      "Comment:": "Says some (usually parenthetical) details about an object -- for instance: its contents.  (Alternatively, see \"print status\" which writes bare bones information about an object.)"
    },
    {
      "Extend pattern:provides:withRules:": [
        "print details",
        [
          {
            "TextList:initially:": [
              "kids",
              {
                "VisibleStorage obj:": {
                  "FromText:": "@obj"
                }
              }
            ]
          }
        ],
        [
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:num:": [
                      {
                        "Len:": {
                          "FromTextList:": "@kids"
                        }
                      },
                      "greater_than",
                      0
                    ]
                  }
                ]
              },
              [
                {
                  "Print response:with:": [
                    "on which are",
                    {
                      "Say:": "{on: .obj} which {are: .obj .kids} {print_inline_objects: .kids}"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "KindOf:is:": [
                      "@obj",
                      "containers"
                    ]
                  },
                  {
                    "Object:field:": [
                      "@obj",
                      "closed"
                    ]
                  }
                ]
              },
              [
                {
                  "Print response:with:": [
                    "closed container",
                    {
                      "Say:": "closed"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "KindOf:is:": [
                      "@obj",
                      "containers"
                    ]
                  }
                ]
              },
              [
                {
                  "Print response:with:": [
                    "empty container",
                    {
                      "Say:": "empty"
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print status",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          },
          {
            "Bool:": "is empty"
          }
        ],
        {
          "Bool:": "skip"
        },
        []
      ]
    },
    {
      "Comment:": "extra information about an object useful for inventorying ( usually appearing inside brackets )"
    },
    {
      "Extend pattern:withRules:": [
        "print status",
        [
          {
            "PatternRule:do:": [
              true,
              [
                {
                  "Print:": {
                    "Spaces do:": [
                      {
                        "Print:": {
                          "Brackets do:": [
                            {
                              "Print:": {
                                "Commas do:": [
                                  {
                                    "PrintInnerStatus obj:isEmpty:": [
                                      {
                                        "FromText:": "@obj"
                                      },
                                      {
                                        "FromBool:": "@is_empty"
                                      }
                                    ]
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Define pattern:requires:result:withRules:": [
        "print inner status",
        [
          {
            "Text:kind:": [
              "obj",
              "object"
            ]
          },
          {
            "Bool:": "is empty"
          }
        ],
        {
          "Bool:": "skip"
        },
        []
      ]
    },
    {
      "Comment:": "usually called via \"print status\" to add spacing and comma-and separators "
    },
    {
      "Extend pattern:withRules:": [
        "print inner status",
        [
          {
            "PatternRule:do:": [
              {
                "Object:field:": [
                  "@obj",
                  "lit"
                ]
              },
              [
                {
                  "Print response:with:": [
                    "lit status",
                    {
                      "Say:": "providing light"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "Object:field:": [
                  "@obj",
                  "worn"
                ]
              },
              [
                {
                  "Print response:with:": [
                    "worn status",
                    {
                      "Say:": "being worn"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "Object:field:": [
                  "@obj",
                  "openable"
                ]
              },
              [
                {
                  "Print response:with:": [
                    "openable status",
                    {
                      "Say:": "{if .obj.closed}closed{else}open{if .is_empty} but empty{end}"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "AllTrue:": [
                  {
                    "Object:field:": [
                      "@obj",
                      "transparent"
                    ]
                  },
                  {
                    "Object:field:": [
                      "@obj",
                      "unopenable"
                    ]
                  }
                ]
              },
              [
                {
                  "Print response:with:": [
                    "transparent status",
                    {
                      "Say:": "{if .is_empty}empty{end}"
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:do:": [
              {
                "Object:field:": [
                  "@obj",
                  "locked"
                ]
              },
              [
                {
                  "Print response:with:": [
                    "locked status",
                    {
                      "Say:": "{if .obj.closed and .obj.locked}locked{end}"
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    }
  ]
}

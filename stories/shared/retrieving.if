{
  "Tapestry:": [
    {
      "Event:action:args dual:": [
        "retrieve from",
        "retrieving from",
        {
          "PairedAction:": "things"
        }
      ]
    },
    {
      "Pattern:provides:rules:": [
        "retrieving from",
        [
          {
            "Text:kind:initially:": [
              "actor",
              "actor",
              {
                "Get object:": {
                  "Object:field:": [
                    "@agent",
                    "pawn"
                  ]
                }
              }
            ]
          }
        ],
        [
          {
            "PatternRule:does:": [
              {
                "IsPlayer obj:": {
                  "FromText:": {
                    "Text value:": "@actor"
                  }
                }
              },
              [
                {
                  "Comment:": "can we \"reach\" the noun we are attempting to retrieve?"
                },
                {
                  "If:from:and:does:else:": [
                    "result",
                    {
                      "FromText:": {
                        "CanReach actor:action:noun:": [
                          {
                            "FromText:": {
                              "Text value:": "@actor"
                            }
                          },
                          {
                            "FromText:": "retrieving from"
                          },
                          {
                            "FromText:": {
                              "Text value:": "@noun"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "Not:": {
                        "Is empty:": "@result"
                      }
                    },
                    [
                      {
                        "Say:": "@result"
                      }
                    ],
                    {
                      "ElseDo does:": [
                        {
                          "If:from:and:does:else:": [
                            "result",
                            {
                              "FromText:": {
                                "CanUse actor:action:noun:otherNoun:": [
                                  {
                                    "FromText:": {
                                      "Text value:": "@actor"
                                    }
                                  },
                                  {
                                    "FromText:": "retrieving from"
                                  },
                                  {
                                    "FromText:": {
                                      "Text value:": "@noun"
                                    }
                                  },
                                  {
                                    "FromText:": {
                                      "Text value:": "@other noun"
                                    }
                                  }
                                ]
                              }
                            },
                            {
                              "Not:": {
                                "Is empty:": "@result"
                              }
                            },
                            [
                              {
                                "Say:": "@result"
                              }
                            ],
                            {
                              "ElseDo does:": [
                                {
                                  "If:does:": [
                                    {
                                      "Send:event:": [
                                        {
                                          "Capture obj:": {
                                            "FromText:": {
                                              "Text value:": "@noun"
                                            }
                                          }
                                        },
                                        {
                                          "RetrieveFrom actor:noun:otherNoun:": [
                                            {
                                              "FromText:": {
                                                "Text value:": "@actor"
                                              }
                                            },
                                            {
                                              "FromText:": {
                                                "Text value:": "@noun"
                                              }
                                            },
                                            {
                                              "FromText:": {
                                                "Text value:": "@other noun"
                                              }
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    [
                                      {
                                        "ReportRetrievingFrom actor:noun:otherNoun:": [
                                          {
                                            "FromText:": {
                                              "Text value:": "@actor"
                                            }
                                          },
                                          {
                                            "FromText:": {
                                              "Text value:": "@noun"
                                            }
                                          },
                                          {
                                            "FromText:": {
                                              "Text value:": "@other noun"
                                            }
                                          }
                                        ]
                                      }
                                    ]
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:rules:": [
        "can reach",
        [
          {
            "PatternRule:does:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:txt:": [
                      "@action",
                      "equal_to",
                      "retrieving from"
                    ]
                  }
                ]
              },
              [
                {
                  "Set variable:from:": [
                    "@response",
                    {
                      "FromText:": {
                        "CanReach actor:action:noun:": [
                          {
                            "FromText:": {
                              "Text value:": "@actor"
                            }
                          },
                          {
                            "FromText:": "taking"
                          },
                          {
                            "FromText:": "@noun"
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:rules:": [
        "can use",
        [
          {
            "PatternRule:does:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:txt:": [
                      "@action",
                      "equal_to",
                      "retrieving from"
                    ]
                  }
                ]
              },
              [
                {
                  "Set variable:from:": [
                    "@response",
                    {
                      "FromText:": {
                        "CanUse actor:action:noun:": [
                          {
                            "FromText:": {
                              "Text value:": "@actor"
                            }
                          },
                          {
                            "FromText:": "taking"
                          },
                          {
                            "FromText:": "@noun"
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            ]
          },
          {
            "PatternRule:does:": [
              {
                "AllTrue:": [
                  {
                    "Cmp:is:txt:": [
                      "@action",
                      "equal_to",
                      "retrieving from"
                    ]
                  },
                  {
                    "Cmp:is:txt:": [
                      {
                        "ParentOf obj:": {
                          "FromText:": {
                            "Text value:": "@noun"
                          }
                        }
                      },
                      "other_than",
                      "@other noun"
                    ]
                  }
                ]
              },
              [
                {
                  "Set variable:from:": [
                    "@response",
                    {
                      "FromText:": {
                        "Response:text:": [
                          "can't retrieve what's not inside",
                          {
                            "RenderTemplate:": "But {they:.noun} {arent:.noun} there now."
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Listen kinds:handlers:": [
        "things",
        [
          {
            "With:event:rules:": [
              "while",
              "retrieve from",
              [
                {
                  "PatternRule:does:": [
                    true,
                    [
                      {
                        "Set variable:from:": [
                          "@success",
                          {
                            "FromBool:": {
                              "Send:event:": [
                                {
                                  "Capture obj:": {
                                    "FromText:": {
                                      "Text value:": "@noun"
                                    }
                                  }
                                },
                                {
                                  "Take actor:noun:": [
                                    {
                                      "FromText:": {
                                        "Text value:": "@actor"
                                      }
                                    },
                                    {
                                      "FromText:": {
                                        "Text value:": "@noun"
                                      }
                                    }
                                  ]
                                }
                              ]
                            }
                          }
                        ]
                      }
                    ]
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    {
      "--": ""
    },
    {
      "Pattern:requires:": [
        "report retrieving from",
        [
          {
            "Text:kind:": [
              "actor",
              "actor"
            ]
          },
          {
            "Text:kind:": [
              "noun",
              "thing"
            ]
          },
          {
            "Text:kind:": [
              "other noun",
              "thing"
            ]
          }
        ]
      ]
    },
    {
      "Pattern:rules:": [
        "report retrieving from",
        [
          {
            "PatternRule:does:": [
              true,
              [
                {
                  "ReportTaking actor:noun:": [
                    {
                      "FromText:": {
                        "Text value:": "@actor"
                      }
                    },
                    {
                      "FromText:": {
                        "Text value:": "@noun"
                      }
                    }
                  ]
                }
              ]
            ]
          }
        ]
      ]
    }
  ]

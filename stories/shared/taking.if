        {
          "PatternRules:": [
            {
              "PatternRule:hook activity:": [
                {
                  "AllTrue:": [
                    {
                      "Cmp:is:txt:": [
                        "@action",
                        {
                          "Equals": []
                        },
                        "taking"
                      ]
                    },
                    {
                      "Cmp:is:txt:": [
                        "@intoObj",
                        {
                          "Equals": []

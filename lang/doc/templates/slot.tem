{{/* a page for all the slots implemented by a command */}}
<h1>{{ Title .Slot.Name }} Slot</h1>
{{ Comment .Slot.Markup }} 
<div>
	Defined by idl <a href="{{ LinkByIdl .Slot.Idl }}">{{ .Slot.Idl }}</a>.
</div>
<section>
	<h3 id="pkg-commands">
		Implemented by:
	</h3>
	<ul class="indexList">
	{{ range  .Commands }}{{ if not .Markup.internal }}
		<li>
			<a class="code" href="{{ LinkByType . }}">{{ .Spec }}</a> {{ Snippet .Markup }}
		</li>
	{{ end }}{{ end }}
	</ul>
</section>

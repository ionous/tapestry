<h1>{{ Title .Slot.Name }} Slot</h1>
{{ Comment .Slot.Markup }} 
<div>
	Defined in: <a href="{{ LinkByIdl .Slot.Idl }}">{{ Pascal .Slot.Idl }}</a>
</div>
<section>
	<h3 id="pkg-commands">
		Implemented by:
	</h3>
	<ul class="indexList">
	{{ range  .Commands }}
		{{if .Slots }}
		<li>
			<a class="code" href="{{ LinkByType . }}">{{ .Spec }}</a> {{ Snippet .Markup }}
		</li>
		{{ end }}
	{{ end }}
	</ul>
</section>
